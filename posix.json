{
  "compiler_path": "g++",
  "compiler_std": "c++20",
  "headers_path": [
    "./dev", "../zt-dev/dev"
  ],
  "sources_path": ["dev/IBKit/*.cc"],
  "output_name": "libIBKit.dylib",
  "compiler_flags": ["-lFoundationKit", "-lGraphicsKit", "-shared", "-fPIC"],
  "cpp_macros": [
    "__IBKIT_DLL__=202401",
    "IBKIT_RELEASE_UUID=$(git rev-parse --abbrev-ref HEAD)-$(uuidgen)"
  ]
}

